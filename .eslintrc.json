{
  "env": {
      "browser": true,
      "es6": true
  },
  "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
  },
  "extends": "airbnb-base",
  "rules": {
    "linebreak-style": 0,
    "no-useless-catch": 0,
    "comma-dangle": 0
  }
}